pipeline {
    agent any 
    stages {
        stage('Build') { 
            steps {
                sh 'echo "new file build"' 
            }
        }
        stage('Test') { 
            steps {
                sh 'echo "test new file"'
            }
        }
        stage('Deploy') { 
            steps {
                sh 'echo "deploy new file"
            }
        }
    }
}